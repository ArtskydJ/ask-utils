{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": ["./libs/"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "plugins": [],
  "opts": {
      "template": "./node_modules/docdash",
      "encoding": "utf8",
      "destination": "docs/",
      "readme": "./README.md",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "scripts": [
      "https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.css",
      "imageproxy?token=cd44e7067699a01270ae73ec7c7b8f89b7f0b4cb406e0e7d57ed6ed9f89f8658&url=https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.js",
      "/scripts/custom.js"
    ]
  }
}